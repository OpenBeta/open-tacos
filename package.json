{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "5.0.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "awesome-debounce-promise": "^2.1.0",
    "axios": "^0.21.1",
    "gatsby": "3.5.1",
    "gatsby-plugin-manifest": "3.5.0",
    "gatsby-plugin-mdx": "^2.5.1",
    "gatsby-plugin-offline": "4.5.1",
    "gatsby-plugin-postcss": "4.5.0",
    "gatsby-plugin-react-helmet": "4.5.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sharp": "3.5.0",
    "gatsby-source-filesystem": "^3.5.0",
    "gatsby-transformer-json": "^3.5.0",
    "query-string": "^7.0.0",
    "react": "17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "recharts": "^2.0.9",
    "slugify": "^1.5.3"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.3.2",
    "autoprefixer": "10.2.5",
    "babel-eslint": "10.1.0",
    "cssnano": "5.0.4",
    "eslint": "7.26.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-react": "7.23.2",
    "gatsby-plugin-eslint": "3.0.0",
    "postcss": "8.3.0",
    "prettier": "2.3.0",
    "tailwindcss": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
}
